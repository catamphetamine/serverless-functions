{
	"presets": ["@babel/preset-env"],

	"plugins":
	[
		["@babel/plugin-transform-runtime", { corejs: 2 }],
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-proposal-class-properties"
	],

  "env":
  {
    "es6":
    {
      "presets":
      [
        ["@babel/preset-env", { modules: false }]
      ]
    }
  }
}