{
  "name": "serverless-functions",
  "version": "0.1.4",
  "description": "A toolkit for writing, testing, running and deploying serverless functions (e.g. AWS Lambda).",
  "main": "index.commonjs.js",
  "module": "index.js",
  "dependencies": {
    "archiver": "^2.1.1",
    "aws-sdk": "^2.288.0",
    "babel-core": "^6.7.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-runtime": "^6.6.1",
    "base64-loader": "^1.0.0",
    "body-parser": "^1.18.3",
    "colors": "^1.3.1",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "filesize": "^3.6.1",
    "fs-extra": "^7.0.0",
    "lodash": "^4.17.10",
    "memory-fs": "^0.4.1",
    "memory-streams": "^0.1.3",
    "source-map-support": "^0.5.8",
    "uuid": "^3.3.2",
    "webpack": "^4.16.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.6.0",
    "cross-env": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.5.0"
  },
  "scripts": {
    "deploy": "babel-node source/tools/aws-lambda/deploy-test.js",
    "test": "echo \"Error: no test specified\" && exit 0",
    "test-coverage": "echo \"Error: no test specified\" && exit 0",
    "clean-for-build": "rimraf ./commonjs/**/* ./modules/**/*",
    "build-commonjs-modules": "cross-env BABEL_ENV=commonjs babel ./source --out-dir ./commonjs --source-maps --ignore *.test.js",
    "build-es6-modules": "cross-env BABEL_ENV=es6 babel ./source --out-dir ./modules --source-maps --ignore *.test.js",
    "build": "npm-run-all clean-for-build build-commonjs-modules build-es6-modules",
    "prepublish": "crlf --set=LF bin/* && npm-run-all build test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/catamphetamine/serverless-functions.git"
  },
  "keywords": [
    "serverless",
    "function",
    "lambda",
    "deploy",
    "aws",
    "amazon"
  ],
  "author": "catamphetamine <purecatamphetamine@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/catamphetamine/serverless-functions/issues"
  },
  "homepage": "https://github.com/catamphetamine/serverless-functions#readme",
  "bin": {
    "serverless": "./bin/serverless.js"
  }
}
